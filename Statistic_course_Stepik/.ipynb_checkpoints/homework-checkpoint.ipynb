   "id": "3629d20c",
   "metadata": {},
   "outputs": [],
   "source": [
    "def ad(arr):\n",
    "    s = 0\n",
    "    for elem in arr:\n",
    "        s += float(elem)\n",
    "    av = s / len(arr)\n",
    "    d = 0\n",
    "    for elem in arr:\n",
    "        d += (int(elem) - av) ** 2\n",
    "    d /= (len(arr) - 1)\n",
    "    ad = d ** 0.5\n",
    "    return ad"
   ]
  },
